package components

type TitleProps struct {
	Title string
}


templ Title(props TitleProps) {
	@TitleStyle()
	<h1 class="title">{ props.Title }</h1>
}

templ TitleStyle() {
	<style>
		.title {
			font-size: "70px";
			color: var(--color-white);
			font-weight: "bold";
			margin-bottom: "12px";
		}		
	</style>
}
