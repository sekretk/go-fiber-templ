package views

import "boy/go-fiber-templ/views/components"
import "boy/go-fiber-templ/views/layout"

templ Main() {
	@layout.Layout(layout.LayoutProps{
		Title: "Find your work",
		MataDescription: "Looking for work",
	}) {
		<main>
			@layout.Header() {
				@components.Title(components.TitleProps{
					Title: "Find your work",
				})
				@components.SubTitle("Simple work search")
			}
		</main>
	}
}
